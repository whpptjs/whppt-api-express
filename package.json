{
  "name": "@whppt/api-express",
  "description": "Whppt api module for express",
  "version": "2.0.234",
  "author": "Svelte Studios (https://www.npmjs.com/settings/svelte-studios/packages)",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "/dist",
    "/types",
    "/cli"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier -c src",
    "lint": "eslint --ext ts --ext js src/ --ignore-path .gitignore",
    "lint:fix": "yarn lint -- --fix",
    "watch": "run-p watch:*",
    "watch:js": "rm -rf dist && rm -rf types && tsc --watch ",
    "build": "run-p build:*",
    "build:js": "rm -rf dist && rm -rf types && tsc && yarn copyFiles",
    "copyFiles": "mkdir -p dist/fonts && cp -r src/routers/Pdf/fonts/* dist/fonts && mkdir -p dist/images && cp -r src/routers/Pdf/images/* dist/images",
    "publish:patch-rc-dry": "yarn test && yarn build && standard-version --release-as patch --prerelease rc --dry-run",
    "publish:patch-rc": "yarn test && yarn build && standard-version --release-as patch --prerelease rc && yarn publish --tag rc && git push --follow-tags",
    "publish:patch-next": "yarn test && yarn build && standard-version --release-as patch --prerelease next && yarn publish --tag next && git push --follow-tags",
    "publish:patch": "yarn build && standard-version --release-as patch && yarn publish && git push --follow-tags",
    "publish:minor": "yarn test && yarn build && standard-version --release-as minor && yarn publish && git push --follow-tags",
    "publish:major": "yarn test && yarn build && standard-version --release-as major && yarn publish && git push --follow-tags",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svelte-studios/whppt-api-express.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svelte-studios/whppt-api-express/issues"
  },
  "bin": {
    "gen-root-user": "cli/generateRootUser.js"
  },
  "homepage": "https://github.com/svelte-studios/whppt-api-express",
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-stage-2": "^7.8.3",
    "@nrwl/jest": "^15.9.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.1",
    "@types/faker": "5.5.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/mongodb": "^4.0.7",
    "@types/node-fetch": "^2.6.2",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.30.4",
    "@types/sinon": "^9.0.11",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.19.0",
    "eslint-config-env": "^23.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^5.5.3",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "lint-staged": "^10.2.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rembrandt": "^0.1.3",
    "sinon": "^9.2.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.180.0",
    "@aws-sdk/client-sesv2": "^3.245.0",
    "axios": "0.27.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "email-syntax": "^2.0.2",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "express-cache-headers": "^0.1.4",
    "express-promise-json-router": "^1.0.8",
    "fast-csv": "^4.3.6",
    "file-type": "^16.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongodb": "^4.8.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2.6.7",
    "nodemailer": "^6.4.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pdfmake": "^0.2.7",
    "prompts": "^2.4.0",
    "require-glob": "^4.1.0",
    "sharp": "^0.30.7",
    "slugify": "^1.4.0",
    "stripe": "^10.15.0",
    "typescript": "^4.7.4",
    "uniqid": "^5.2.0",
    "uri-js": "^4.2.2",
    "xero-node": "^4.34.0"
  }
}
